export PATH=/usr/local/share/npm/bin:/usr/local/bin:$HOME/.rbenv/bin:$PATH

if 
  which rbenv > /dev/null; then eval "$(rbenv init -)"; 
fi

# Enable color by default in Terminal output
export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced

